			</ReadingType>
			
			<IntervalBlock> 
				<interval>
					<duration>2678400</duration>
					<start>2017-01-01T00:00:00+02:00</start>
				</interval>
			
                <!-- 
                Messwert 0,0 Wh.
                Summe seit Start des TAF-2
                -->
				<IntervalReading>
					<espi:value>0</espi:value>			
					<timePeriod>
						<duration>0</duration>
						<start>2017-02-01T00:00:00+02:00</start>
					</timePeriod>
                    <!-- Statuswort mach FNN -->
                    <!-- SMGW-Status und Zählerstatus in HEX Notation-->
